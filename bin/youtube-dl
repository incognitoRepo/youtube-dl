#!/Users/alberthan/VSCodeProjects/vytd/bin/ python3
import youtube_dl
import hunter
from youtube_dl.hunterconfig import QueryConfig
from hunter.tracer import Tracer
from pathlib import Path
from pdb import set_trace as st
import os
import pickle, sys

test = False

if __name__ == '__main__':
  if test:
    qc = QueryConfig()
    qcfg = [qc.eventpickle()]#,qc.noloop()]
    tracer = Tracer()
    for query,actions,outputs,filenames,write_func,evtdcts_pklpth in qcfg:
      tracer.trace(query)
      youtube_dl.main()
      write_func()
      action = actions[0]
      for e in action.evt_dcts:
        try:
          pickle.dumps(e,pickle.HIGHEST_PROTOCOL)
        except:
          st()
      with open('evts.pkl','wb') as f:
        pickle.dump(action.evt_dcts,f,pickle.HIGHEST_PROTOCOL)
      with open('evts.pkl','rb') as f:
        evts = pickle.load(f)
  else:
    youtube_dl.main()
