#!/usr/bin/env python3
import youtube_dl
import hunter
from youtube_dl.hunterconfig import QueryConfig
from hunter.tracer import Tracer
from pathlib import Path
from pdb import set_trace as st
import os
import sys

if __name__ == '__main__':
  qc = QueryConfig()
  qcfg = [qc.targetfunc()]#,qc.noloop()]
  tracer = Tracer()
  for query,actions,outputs,filenames,write_func,pkldf in qcfg:
    tracer.trace(query)
    youtube_dl.main()
    write_func()
